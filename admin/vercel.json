{
  "version": 2,
  "buildCommand": "cd admin && npm run build",
  "outputDirectory": "admin/.next",
  "installCommand": "cd admin && npm install",
  "routes": [
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "admin/app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
